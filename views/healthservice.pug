                .h4 UCAS Points
                    span#spiderSpan2
                input.slider.mbs#slider2(type="range" min="72" max="144" value="50")

                .h4 Feature 3
                    span#spiderSpan3
                input.slider.mbs#slider3(type="range" min="1" max="100" value="50")

                .h4 Feature 4
                    span#spiderSpan4
                input.slider.mbs#slider4(type="range" min="1" max="100" value="50")

                .h4 Feature 5
                    span#spiderSpan5
                input.slider.mbs#slider5(type="range" min="1" max="100" value="50")

                .h4 Feature 6
                select.optionpicker.mbs
                    option(value="aa") aa
                    option(value="bb") bb
                    option(value="cc") cc

        .col-sm-9
